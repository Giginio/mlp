    "# with try and catch\n",
    "# Set options for Chrome-Driver (headless mode)\n",
    "opts = Options()\n",
    "opts.add_argument(\"--headless\")\n",
    "opts.add_argument(\"user-agent=\" + random.choice(ua_list))\n",
