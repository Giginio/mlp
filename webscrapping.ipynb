    "\n",
    "from selenium import webdriver\n",
    "from selenium.webdriver.chrome.service import Service\n",
    "from selenium.webdriver.chrome.options import Options #for user Agents\n",
    "\n",
    "# Specify the path to your ChromeDriver executable\n",
    "chrome_driver_path = r'C:\\Users\\Pierluigi\\anaconda3\\envs\\adsenv\\AdvancedDataScience\\SW2_Advanced_Data_Collection_Methods\\chromedriver\\chromedriver.exe'\n",
    "\n",
    "# Initialize the Service object with the path to ChromeDriver\n",
    "service = Service(executable_path=chrome_driver_path)\n",
    "\n",
    "# Now you can pass the service object to the webdriver.Chrome constructor\n",
    "driver = webdriver.Chrome(service=service, options=opts)\n",
    "\n",
    "\n",
    "\n",
    "#opts = Options()\n",
