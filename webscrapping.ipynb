    "# Initialize the Service object with the path to ChromeDriver\n",
    "service = Service(executable_path=chrome_driver_path)\n",
    "\n",
    "# Now you can pass the service object to the webdriver.Chrome constructor\n",
    "driver = webdriver.Chrome(service=service, options=opts)\n",
    "\n",
    "\n",
    "\n",
    "#opts = Options()\n",
    "# Example of setting a user-agent string to Chrome 100\n",
    "#opts.add_argument(\"user-agent=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/100.0.0.0 Safari/537.36\")\n",
    "\n",
    "#driver = webdriver.Chrome(options=opts)\n",
    "\n",
    "# Chrome-Driver\n",
    "driver = webdriver.Chrome(service=service, options=opts)\n",
    "\n",
    "page = 1\n",
